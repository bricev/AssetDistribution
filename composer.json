    "homepage": "http://libcast.com",
    "license": "MIT",
    "require": {
        "symfony/http-foundation": "~3.0",
        "doctrine/cache": "~1.0",
        "league/flysystem": "~1.0",
        "dailymotion/sdk": "~1.6",
        "google/apiclient": "v2.0.0-RC5",
        "vimeo/vimeo-api": "~1.2",
        "psr/log": "~1.0"
    },
    "require-dev": {
